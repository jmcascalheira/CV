@article{ ISI:000437144800002,
Author = {Bicho, Nuno and Cascalheira, Joao and Andre, Lino and Haws, Jonathan and
   Gomes, Ana and Goncalves, Celia and Raja, Mussa and Benedetti, Michael},
Title = {{Portable art and personal ornaments from Txina-Txina: a new Later Stone
   Age site in the Limpopo River Valley, southern Mozambique}},
Journal = {{ANTIQUITY}},
Year = {{2018}},
Volume = {{92}},
Number = {{363}},
Month = {{JUN}},
Abstract = {{This paper reports on preliminary fieldwork at the Later Stone Age site
   of Txina-Txina in Mozambique. Excavation yielded a long stratigraphic
